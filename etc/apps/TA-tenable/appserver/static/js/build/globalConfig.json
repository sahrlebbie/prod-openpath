                    ]
                },
                {
                    "name": "tenable_securitycenter_mobile", 
                    "title": "Tenable.sc Mobile", 
                    "entity": [
                        {
                            "validators": [
                                {
                                    "pattern": "^[a-zA-Z]\\w*$", 
