                        {
                            "field": "global_account", 
                            "required": true, 
                            "type": "singleSelect", 
                            "options": {
                                "referenceName": "account"
                            }, 
                            "label": "Global Account", 
                            "help": ""
                        },
                        {
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "type": "string", 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "maxLength": 8192
                                }
                            ], 
                            "type": "text", 
                            "label": "Query Name", 
                            "field": "query_name", 
                            "help": "Enter the query name for Tenable.sc mobile filter.", 
                            "required": false
                        }
                    ]
                }
            ], 
            "title": "Inputs", 
            "table": {
