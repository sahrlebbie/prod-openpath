                            "field": "start_time", 
                            "label": "Start Time"
                        }, 
                        {
                            "label": "Lowest Severity to Store", 
                            "type": "singleSelect", 
                            "options": {
                                "disableSearch": true, 
                                "autoCompleteFields": [
                                    {
                                        "label": "Info", 
                                        "value": "info"
                                    }, 
                                    {
                                        "label": "Low", 
                                        "value": "low"
                                    }, 
                                    {
                                        "label": "Medium", 
                                        "value": "medium"
                                    }, 
                                    {
                                        "label": "High", 
                                        "value": "high"
                                    }, 
                                    {
                                        "label": "Critical", 
                                        "value": "critical"
                                    }
                                ]
                            }, 
                            "field": "lowest_severity_to_store", 
                            "defaultValue": "info", 
                            "help": "The Lowest Vulnerability severity to store.", 
                            "required": true
                        },
                        {
                            "help": "Sync plugin details via this input? ", 
                            "type": "checkbox", 
                            "label": "Sync Plugin Details", 
                            "field": "sync_plugins", 
                            "required": false
                        },
                        {
                            "help": "Historical Fixed Vulnerability via this input?", 
                            "type": "checkbox", 
                            "label": "Historical Fixed Vulnerability", 
                            "field": "fixed_vulnerability", 
                            "required": false
                        },
                        {
                            "required": false, 
                            "help": "e.g. {\"tag_key1\": [\"tag_value1\", \"tag_value2\"...], \"tag_key2\": ...} OR [(\"key1\", \"value1\"), (\"key2\", \"value2\"), ...] format.",
                            "type": "text", 
                            "field": "tags", 
                            "label": "Tags"
                        }
                    ]
                }, 
