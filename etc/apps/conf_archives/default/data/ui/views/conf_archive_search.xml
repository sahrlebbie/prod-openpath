      </single>
      <single>
        <search>
          <query>| inputlookup conf_breakout_sessions.csv 
            | fillnull value="N/A"
            | search $yr$ $title$ $description$ $speaker$ $solutions$ $industries$ $product$ $role$ $track$ $focus$ $level$ $other$ $type$ Type="Panel Discussion"
            | stats max(*) AS * by Year, Title, Description
            | stats count</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">block</option>
        <option name="drilldown">all</option>
        <option name="height">100</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x4B0082","0x4B0082"]</option>
        <option name="rangeValues">[0]</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="underLabel">PANEL DISCUSSIONS</option>
        <option name="unitPosition">after</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
        <drilldown>
          <set token="type">Type="Panel Discussion"</set>
        </drilldown>
      </single>
      <single>
        <search>
          <query>| inputlookup conf_breakout_sessions.csv 
            | fillnull value="N/A"
            | search $yr$ $title$ $description$ $speaker$ $solutions$ $industries$ $product$ $role$ $track$ $focus$ $level$ $other$ $type$ Type="Splunk Snack"
            | stats max(*) AS * by Year, Title, Description
            | stats count</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">block</option>
        <option name="drilldown">all</option>
        <option name="height">100</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x8B00FF","0x8B00FF"]</option>
        <option name="rangeValues">[0]</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="underLabel">SPLUNK SNACKS</option>
        <option name="unitPosition">after</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
        <drilldown>
          <set token="type">Type="Splunk Snack"</set>
        </drilldown>
      </single>
      <single>
        <search>
          <query>| inputlookup conf_breakout_sessions.csv 
            | fillnull value="N/A"
            | search $yr$ $title$ $description$ $speaker$ $solutions$ $industries$ $product$ $role$ $track$ $focus$ $level$ $other$ $type$ Type="Super Session"
            | stats max(*) AS * by Year, Title, Description
            | stats count</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">block</option>
        <option name="drilldown">all</option>
        <option name="height">100</option>
