<dashboard>
  <label>Battery with Percentages</label>
  <description>Simple and complex macro examples</description>
  <row>
    <panel>
      <title>Simple</title>
      <viz type="svg.svg">
        <search>
          <query>| makeresults 
| eval percentage=0
| `svg_viz_battery_percentage_simple(percentage)`</query>
          <earliest>-1m</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">none</option>
        <option name="height">50</option>
        <option name="svg.svg.debug">false</option>
        <option name="svg.svg.viewbox_height">50</option>
        <option name="svg.svg.viewbox_min_x">0</option>
        <option name="svg.svg.viewbox_min_y">0</option>
        <option name="svg.svg.viewbox_width">100</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
      </viz>
    </panel>
