<?xml version="1.0" encoding="UTF-8"?>
<CmConfig xmlns="http://purl.org/cloudmeter/config" version="7.2.0">
  <Vocabulary id="ldap">
    <Name>LDAP Protocol Vocabulary</Name>
    <Locked>true</Locked>
    <Term id="ldap.event">
      <Type>object</Type>
      <Comment>Event representing a LDAP event</Comment>
    </Term>
    <Term id="ldap.assertion-value">
      <Type>string</Type>
      <Comment>Filter expression second operand, which is an assertion value</Comment>
    </Term>
    <Term id="ldap.attribute-description">
      <Type>shortstring</Type>
      <Comment>Filter expression first operand, which is an attribute description</Comment>
    </Term>
    <Term id="ldap.contains-sasl">
      <Type>uint32</Type>
      <Comment>Indicates whether the authentication is done using SASL mechanism</Comment>
    </Term>
    <Term id="ldap.hostname">
      <Type>shortstring</Type>
      <Comment>Hostname extracted from a logon response to a CLDAP searchRequest</Comment>
    </Term>
    <Term id="ldap.message-id">
      <Type>uint32</Type>
      <Comment>Message identification</Comment>
    </Term>
    <Term id="ldap.message-type">
      <Type>shortstring</Type>
      <Comment>Message type</Comment>
    </Term>
    <Term id="ldap.elements">
      <Type>string</Type>
      <Comment>LDAP element, map containing name-value pairs with nested elements</Comment>
    </Term>
  </Vocabulary>
</CmConfig>
