<?xml version="1.0" encoding="UTF-8"?>
<CmConfig xmlns="http://purl.org/cloudmeter/config" version="7.2.0">
	<Vocabulary id="time">
		<Name>Time</Name>
		<Comment>Vocabulary for describing time metrics.</Comment>
		<Locked>true</Locked>
		<Term id="time.date">
			<Type format="%Y-%m-%d">date</Type>
			<Comment>Date value (2008-07-20)</Comment>
		</Term>
		<Term id="time.time">
			<Type format="%H:%M:%S">time</Type>
			<Comment>Time value (12:20:21)</Comment>
		</Term>
		<Term id="time.date-time">
			<Type format="%Y-%m-%d %H:%M:%S">datetime</Type>
			<Comment>Date and time value (2008-07-20 12:20:21)</Comment>
		</Term>
		<Term id="time.iso8601">
			<Type format="%Y-%m-%dT%H:%M:%S%FZ">datetime</Type>
			<Comment>ISO 8601 with fractional seconds for start of event (2014-05-05T21:35:56.213Z)</Comment>
		</Term>
		<Term id="time.end-time">
			<Type format="%Y-%m-%dT%H:%M:%S%FZ">datetime</Type>
			<Comment>ISO 8601 with fractional seconds for end of event (2014-05-05T21:35:56.213Z)</Comment>
		</Term>
		<Term id="time.epoch-time">
			<Type>uint32</Type>
			<Comment>Number of seconds since the epoch (January 1, 1970)</Comment>
		</Term>
        <Term id="time.epoch-time-end">
            <Type>uint32</Type>
            <Comment>End time of a flow expressed as number of seconds since the epoch (January 1, 1970)</Comment>
        </Term>
		<Term id="time.clf-date">
			<Type format="%d/%b/%Y:%H:%M:%S %q">datetime</Type>
			<Comment>Date and time value (CLF format)</Comment>
		</Term>
		<Term id="time.request-start-time">
			<Type format="%Y%m%dT%H%M%S%F">datetime</Type>
			<Comment>Date and time of the first request packet (20080720T122021.123213)</Comment>
		</Term>
		<Term id="time.request-end-time">
			<Type format="%Y%m%dT%H%M%S%F">datetime</Type>
			<Comment>Date and time of the last request packet (20080720T122021.123213)</Comment>
		</Term>
		<Term id="time.response-start-time">
			<Type format="%Y%m%dT%H%M%S%F">datetime</Type>
			<Comment>Date and time of the first response packet (20080720T122021.123213)</Comment>
		</Term>
		<Term id="time.response-end-time">
			<Type format="%Y%m%dT%H%M%S%F">datetime</Type>
			<Comment>Date and time of the last response packet (20080720T122021.123213)</Comment>
		</Term>
	</Vocabulary>
</CmConfig>
