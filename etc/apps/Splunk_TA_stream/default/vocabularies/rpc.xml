<?xml version="1.0" encoding="UTF-8"?>
<CmConfig xmlns="http://purl.org/cloudmeter/config" version="7.2.0">
  <Vocabulary id="rpc">
    <Locked>true</Locked>
    <Name>RPC Protocol Vocabulary</Name>
    <Term id="rpc.event">
      <Type>object</Type>
      <Comment>Event representing a rpc command</Comment>
    </Term>
    <Term id="rpc.message-type">
      <Type>shortstring</Type>
      <Comment>Message type (Call or Reply)</Comment>
    </Term>
    <Term id="rpc.procedure">
      <Type>uint32</Type>
      <Comment>Contains the request used by the application program</Comment>
    </Term>
    <Term id="rpc.program">
      <Type>uint32</Type>
      <Comment>Program Identifier</Comment>
    </Term>
    <Term id="rpc.program-version">
      <Type>uint32</Type>
      <Comment>Version of the used program over RPC</Comment>
    </Term>
    <Term id="rpc.state">
      <Type>uint32</Type>
      <Comment>Status of the command response</Comment>
    </Term>
    <Term id="rpc.version">
      <Type>uint32</Type>
      <Comment>Version of the RPC protocol</Comment>
    </Term>
    <Term id="rpc.xid">
      <Type>uint32</Type>
      <Comment>Identifier of the request or the reply</Comment>
    </Term>
  </Vocabulary>
</CmConfig>
