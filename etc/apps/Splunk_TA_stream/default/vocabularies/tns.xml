<?xml version="1.0" encoding="UTF-8"?>
<CmConfig xmlns="http://purl.org/cloudmeter/config" version="7.2.0">
  <Vocabulary id="tns">
    <Locked>true</Locked>
    <Name>TNS (Oracle) Protocol Vocabulary</Name>
    <Term id="tns.event">
      <Type>object</Type>
      <Comment>Event representing a TNS event</Comment>
    </Term>
    <Term id="tns.dbname">
      <Type>shortstring</Type>
      <Comment>Name of accessed database</Comment>
    </Term>
    <Term id="tns.client-hostname">
      <Type>shortstring</Type>
      <Comment>Client machine hostname</Comment>
    </Term>
    <Term id="tns.client-os">
      <Type>shortstring</Type>
      <Comment>Client machine operating system</Comment>
    </Term>
    <Term id="tns.client-program-name">
      <Type>shortstring</Type>
      <Comment>Client program name</Comment>
    </Term>
    <Term id="tns.client-program-path">
      <Type>shortstring</Type>
      <Comment>Client program absolute path</Comment>
    </Term>
    <Term id="tns.login">
      <Type>shortstring</Type>
      <Comment>User's login string</Comment>
    </Term>
    <Term id="tns.password">
      <Type>shortstring</Type>
      <Comment>User's password string</Comment>
    </Term>
    <Term id="tns.query">
      <Type>string</Type>
      <Comment>Database query</Comment>
    </Term>
    <Term id="tns.server-hostname">
      <Type>shortstring</Type>
      <Comment>Database server hostname</Comment>
    </Term>
    <Term id="tns.server-os">
      <Type>shortstring</Type>
      <Comment>Database server operating system</Comment>
    </Term>
    <Term id="tns.version">
      <Type>uint16</Type>
      <Comment>Version number of Oracle server</Comment>
    </Term>
    <Term id="tns.mtu">
      <Type>uint16</Type>
      <Comment>Maximum Transmission data Unit size</Comment>
    </Term>
  </Vocabulary>
</CmConfig>
