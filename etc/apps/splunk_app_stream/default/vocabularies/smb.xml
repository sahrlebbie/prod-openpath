<?xml version="1.0" encoding="UTF-8"?>
<CmConfig xmlns="http://purl.org/cloudmeter/config" version="7.2.0">
  <Vocabulary id="smb">
    <Name>SMB Protocol Vocabulary</Name>
    <Locked>true</Locked>
    <Term id="smb.event">
      <Type>object</Type>
      <Comment>Event representing an SMB event</Comment>
    </Term>
    <Term id="smb.login">
      <Type>shortstring</Type>
      <Comment>User's login string</Comment>
    </Term>
    <Term id="smb.command">
      <Type>shortstring</Type>
      <Comment>Command string</Comment>
    </Term>
    <Term id="smb.dialect">
      <Type>shortstring</Type>
      <Comment>The version of the SMB Protocol</Comment>
    </Term>
    <Term id="smb.domain">
      <Type>shortstring</Type>
      <Comment>Domain name</Comment>
    </Term>
    <Term id="smb.filename">
      <Type>shortstring</Type>
      <Comment>Name of the transferred file</Comment>
    </Term>
    <Term id="smb.filesize">
      <Type>uint64</Type>
      <Comment>Size (byte) of the transferred file</Comment>
    </Term>
    <Term id="smb.native-os">
      <Type>shortstring</Type>
      <Comment>Client's operating system</Comment>
    </Term>
    <Term id="smb.nt-status">
      <Type>uint32</Type>
      <Comment>NT error code</Comment>
    </Term>
    <Term id="smb.path">
      <Type>shortstring</Type>
      <Comment>The server/share name of the resource to which the client attempts to connect</Comment>
    </Term>
    <Term id="smb.search-attributes">
      <Type>uint32</Type>
      <Comment>An attribute mask used to specify the standard attributes a file must have in order to match the search</Comment>
    </Term>
    <Term id="smb.search-pattern">
      <Type>shortstring</Type>
      <Comment>The file pattern to search for</Comment>
    </Term>
    <Term id="smb.service">
      <Type>shortstring</Type>
      <Comment>The type of resource that the client intends to access</Comment>
    </Term>
    <Term id="smb.user-id">
      <Type>uint32</Type>
      <Comment>User identifier (SMB usmb_v1 only)</Comment>
    </Term>
  </Vocabulary>
</CmConfig>
