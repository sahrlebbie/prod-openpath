<?xml version="1.0" encoding="UTF-8"?>
<CmConfig xmlns="http://purl.org/cloudmeter/config" version="7.2.0">
  <Vocabulary id="sip">
    <Name>SIP protocol Vocabulary</Name>
    <Locked>true</Locked>
    <Term id="sip.event">
      <Type>object</Type>
      <Comment>Event representing SIP message</Comment>
    </Term>
    <Term id="sip.accept-language">
      <Type>shortstring</Type>
      <Comment>Indicates the preferred languages</Comment>
    </Term>
    <Term id="sip.alert-info">
      <Type>shortstring</Type>
      <Comment>Specifies an alternative ring tone</Comment>
    </Term>
    <Term id="sip.call-duration">
      <Type>uint32</Type>
      <Comment>Call duration in microseconds</Comment>
    </Term>
    <Term id="sip.call-id">
      <Type>shortstring</Type>
      <Comment>Call id, extracted for each call</Comment>
    </Term>
    <Term id="sip.call-info">
      <Type>shortstring</Type>
      <Comment>Provides additional information about the caller or callee</Comment>
    </Term>
    <Term id="sip.callee">
      <Type>shortstring</Type>
      <Comment>Contains the identity of the called party for a call</Comment>
    </Term>
    <Term id="sip.callee-addr">
      <Type>shortstring</Type>
      <Comment>IPv4 address which could be used by the called party</Comment>
    </Term>
    <Term id="sip.callee-addr-v6">
      <Type>shortstring</Type>
      <Comment>IPv6 address which could be used by the called party</Comment>
    </Term>
    <Term id="sip.callee-domain">
      <Type>shortstring</Type>
      <Comment>Callee's domain</Comment>
    </Term>
     <Term id="sip.callee-e164">
      <Type>shortstring</Type>
      <Comment>Format of the callee's telephone numbers</Comment>
    </Term>
    <Term id="sip.callee-nickname">
      <Type>shortstring</Type>
      <Comment>Callee nickname</Comment>
    </Term>
    <Term id="sip.callee-port">
      <Type>uint16</Type>
      <Comment>Port which could be used by the callee</Comment>
    </Term>
    <Term id="sip.callee-server-agent">
      <Type>shortstring</Type>
      <Comment>Server's software in the callee way</Comment>
    </Term>
    <Term id="sip.callee-user-agent">
      <Type>shortstring</Type>
      <Comment>Client's software used by the callee</Comment>
    </Term>
    <Term id="sip.callee-user-phone">
      <Type>int32</Type>
      <Comment>Callee's phone presence flag</Comment>
    </Term>
    <Term id="sip.caller">
      <Type>shortstring</Type>
      <Comment>Contains the identity of the initiator of the call</Comment>
    </Term>
    <Term id="sip.caller-addr">
      <Type>shortstring</Type>
      <Comment>IPv4 address which could be used by the initiator of the call</Comment>
    </Term>
    <Term id="sip.caller-addr-v6">
      <Type>shortstring</Type>
      <Comment>IPv6 address which could be used by the initiator of the call</Comment>
    </Term>
    <Term id="sip.caller-domain">
      <Type>shortstring</Type>
      <Comment>Caller's domain</Comment>
    </Term>
    <Term id="sip.caller-e164">
      <Type>shortstring</Type>
      <Comment>Format of the caller's telephone numbers</Comment>
    </Term>
    <Term id="sip.caller-nickname">
      <Type>shortstring</Type>
      <Comment>Caller nickname</Comment>
    </Term>
    <Term id="sip.caller-port">
      <Type>uint16</Type>
      <Comment>Port which could be used by the caller.</Comment>
    </Term>
    <Term id="sip.caller-server-agent">
      <Type>shortstring</Type>
      <Comment>Server's software in the caller way.</Comment>
    </Term>
    <Term id="sip.caller-user-agent">
      <Type>shortstring</Type>
      <Comment>Client's software in the caller way</Comment>
    </Term>
    <Term id="sip.caller-user-phone">
      <Type>int32</Type>
      <Comment>Caller's phone presence flag</Comment>
    </Term>
    <Term id="sip.confcall-callee">
      <Type>shortstring</Type>
      <Comment>Callee's name, in a confcall</Comment>
    </Term>
    <Term id="sip.confcall-caller">
      <Type>shortstring</Type>
      <Comment>Caller's name, in a confcall</Comment>
    </Term>
    <Term id="sip.connection-info-addr">
      <Type>shortstring</Type>
      <Comment>Connection IPv4 address</Comment>
    </Term>
    <Term id="sip.connection-info-addr-type">
      <Type>shortstring</Type>
      <Comment>Connection address type</Comment>
    </Term>
    <Term id="sip.connection-info-addr-v6">
      <Type>shortstring</Type>
      <Comment>Connection IPv6 address</Comment>
    </Term>
    <Term id="sip.connection-info-net-type">
      <Type>shortstring</Type>
      <Comment>Network type for the connection</Comment>
    </Term>
    <Term id="sip.contact">
      <Type>shortstring</Type>
      <Comment>The Contact header field provides a SIP or SIPS URI that can be used to contact that specific instance of the UA for subsequent requests</Comment>
    </Term>
    <Term id="sip.cseq">
      <Type>shortstring</Type>
      <Comment>Sequence number</Comment>
    </Term>
    <Term id="sip.data-port">
      <Type>uint16</Type>
      <Comment>Data port for client's protocol</Comment>
    </Term>
    <Term id="sip.date">
      <Type>shortstring</Type>
      <Comment>Contains the date and time</Comment>
    </Term>
    <Term id="sip.end-status">
      <Type>shortstring</Type>
      <Comment>Status of the call end</Comment>
    </Term>
    <Term id="sip.from">
      <Type>shortstring</Type>
      <Comment>The initiator of the request</Comment>
    </Term>
    <Term id="sip.from-tag">
      <Type>blob</Type>
      <Comment>A globally unique id of the caller</Comment>
    </Term>
    <Term id="sip.media-attr">
      <Type>string</Type>
      <Comment>Media attributes</Comment>
    </Term>
    <Term id="sip.media-format">
      <Type>uint32</Type>
      <Comment>Client's protocol formats available</Comment>
    </Term>
    <Term id="sip.media-proto">
      <Type>shortstring</Type>
      <Comment>Protocol used in client stream</Comment>
    </Term>
    <Term id="sip.media-type">
      <Type>shortstring</Type>
      <Comment>Contains the media type</Comment>
    </Term>
    <Term id="sip.method">
      <Type>shortstring</Type>
      <Comment>The command</Comment>
    </Term>
    <Term id="sip.mime-type">
      <Type>shortstring</Type>
      <Comment>Data type</Comment>
    </Term>
    <Term id="sip.p-asserted-id">
      <Type>shortstring</Type>
      <Comment>Indicates the identity of the trusted SIP server</Comment>
    </Term>
    <Term id="sip.proxy-authorization">
      <Type>shortstring</Type>
      <Comment>Allows the client to identify itself (or its user) to a proxy that requires authentication</Comment>
    </Term>
    <Term id="sip.reason">
      <Type>shortstring</Type>
      <Comment>The reason a Session Initiation Protocol request was issued</Comment>
    </Term>
    <Term id="sip.record-route">
      <Type>shortstring</Type>
      <Comment>The Record-Route header field is inserted by proxies in a request to force future requests in the dialog to be routed through the proxy</Comment>
    </Term>
    <Term id="sip.remote-party-id">
      <Type>shortstring</Type>
      <Comment>The IP address of the remote party</Comment>
    </Term>
    <Term id="sip.reply-code">
      <Type>shortstring</Type>
      <Comment>Return status code</Comment>
    </Term>
    <Term id="sip.request-call-id">
      <Type>shortstring</Type>
      <Comment>Call's id extracted for each sip request</Comment>
    </Term>
    <Term id="sip.server-agent">
      <Type>shortstring</Type>
      <Comment>Server's software</Comment>
    </Term>
    <Term id="sip.session-duration">
      <Type>uint32</Type>
      <Comment>Session duration in microseconds</Comment>
    </Term>
    <Term id="sip.setup-delay">
      <Type>uint32</Type>
      <Comment>Call setup delay in microseconds</Comment>
    </Term>
    <Term id="sip.start-time">
      <Type format="%Y/%m/%d %H:%M:%S">datetime</Type>
      <Comment>Start date of the call</Comment>
    </Term>
     <Term id="sip.subject">
      <Type>shortstring</Type>
      <Comment>The subject header present in the SIP packet</Comment>
    </Term>
     <Term id="sip.time-before-spk">
      <Type>uint32</Type>
      <Comment>Waiting delay before speak in microseconds</Comment>
    </Term>
     <Term id="sip.to">
      <Type>shortstring</Type>
      <Comment>The recipient of the request</Comment>
    </Term>
     <Term id="sip.to-tag">
      <Type>blob</Type>
      <Comment>A globally unique id of the callee</Comment>
    </Term>
    <Term id="sip.uri">
      <Type>shortstring</Type>
      <Comment>Contains the URI (similar to To: field)</Comment>
    </Term>
    <Term id="sip.user-agent">
      <Type>shortstring</Type>
      <Comment>Client's software</Comment>
    </Term>
    <Term id="sip.user-id">
      <Type>shortstring</Type>
      <Comment>Client identifier used for his registering with a SIP server</Comment>
    </Term>
    <Term id="sip.via">
      <Type>shortstring</Type>
      <Comment>The Via header field indicates the transport used for the transaction and identifies the location where the response is to be sent</Comment>
    </Term>
    <Term id="sip.www-authenticate">
      <Type>shortstring</Type>
      <Comment>Contains an authentication challenge</Comment>
    </Term>
  </Vocabulary>
</CmConfig>
