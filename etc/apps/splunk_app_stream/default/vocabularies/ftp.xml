<?xml version="1.0" encoding="UTF-8"?>
<CmConfig xmlns="http://purl.org/cloudmeter/config" version="7.2.0">
  <Vocabulary id="ftp">
    <Name>FTP Protocol Vocabulary</Name>
    <Locked>true</Locked>
    <Term id="ftp.event">
      <Type>object</Type>
      <Comment>Event representing an FTP event</Comment>
    </Term>
    <Term id="ftp.login">
      <Type>shortstring</Type>
      <Comment>User's login string</Comment>
    </Term>
    <Term id="ftp.loadway">
      <Type>shortstring</Type>
      <Comment>The file transfer way (Upload vs Download)</Comment>
    </Term>
    <Term id="ftp.method">
      <Type>shortstring</Type>
      <Comment>Contains the FTP command sent</Comment>
    </Term>
    <Term id="ftp.method-parameter">
      <Type>shortstring</Type>
      <Comment>Method parameter</Comment>
    </Term>
    <Term id="ftp.filename">
      <Type>string</Type>
      <Comment>Name of the transferred file</Comment>
    </Term>
    <Term id="ftp.filesize">
      <Type>uint32</Type>
      <Comment>Size (byte) of the transferred file</Comment>
    </Term>
    <Term id="ftp.data-port">
      <Type>uint16</Type>
      <Comment>Data connection TCP port</Comment>
    </Term>
    <Term id="ftp.content-type">
      <Type>shortstring</Type>
      <Comment>The content type of transferred file</Comment>
    </Term>
    <Term id="ftp.greeting-message">
      <Type>string</Type>
      <Comment>First line of the server banner</Comment>
    </Term>
    <Term id="ftp.index">
      <Type>uint32</Type>
      <Comment>Identifier of the request and response in a FTP flow.</Comment>
    </Term>
    <Term id="ftp.offset">
      <Type>uint32</Type>
      <Comment>Start offset of the file transfer</Comment>
    </Term>
    <Term id="ftp.password">
      <Type>shortstring</Type>
      <Comment>User's password string</Comment>
    </Term>
    <Term id="ftp.reply-code">
      <Type>uint16</Type>
      <Comment>FTP server reply code</Comment>
    </Term>
    <Term id="ftp.reply-content">
      <Type>shortstring</Type>
      <Comment>FTP server response message content</Comment>
    </Term>
    <Term id="ftp.inherent-parent">
      <Type>blob</Type>
      <Comment>Parent inheritance key, stored in an hashtable and kept until parent session expiration.</Comment>
    </Term>
    <Term id="ftp.transfer-duration">
      <Type>uint32</Type>
      <Comment>File transfer duration in microseconds</Comment>
    </Term>
  </Vocabulary>
</CmConfig>
