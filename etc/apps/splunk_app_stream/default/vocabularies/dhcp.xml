<?xml version="1.0" encoding="UTF-8"?>
<CmConfig xmlns="http://purl.org/cloudmeter/config" version="7.2.0">
    <Vocabulary id="dhcp">
    	<Name>DHCP Protocol Vocabulary</Name>
        <Locked>true</Locked>
        <Term id="dhcp.event">
            <Type>object</Type>
            <Comment>Event representing DHCP</Comment>
        </Term>
        <Term id="dhcp.message-type">
            <Type>shortstring</Type>
            <Comment>The type of DHCP message</Comment>
        </Term>
        <Term id="dhcp.transaction-id">
            <Type>uint32</Type>
            <Comment>Transaction ID used to associate requests and responses</Comment>
        </Term>
        <Term id="dhcp.filename">
            <Type>shortstring</Type>
            <Comment>Name of boot file used during initialization</Comment>
        </Term>
        <Term id="dhcp.client-mac">
            <Type>shortstring</Type>
            <Comment>Mac address of client</Comment>
        </Term>
        <Term id="dhcp.current-client-ip">
            <Type>shortstring</Type>
            <Comment>Client's current IP address</Comment>
        </Term>
        <Term id="dhcp.dns-ip">
            <Type>shortstring</Type>
            <Comment>DNS server's ip</Comment>
        </Term>
        <Term id="dhcp.relay-ip">
            <Type>shortstring</Type>
            <Comment>Relay agent IP address  (used when booting via a relay agent).</Comment>
        </Term>
        <Term id="dhcp.lease-time">
            <Type>uint32</Type>
            <Comment>Specifies lease time DHCP server is willing to offer</Comment>
        </Term>
        <Term id="dhcp.server-ip">
            <Type>shortstring</Type>
            <Comment>IP address of the next server (used when booting via a server)</Comment>
        </Term>
        <Term id="dhcp.server-name">
            <Type>shortstring</Type>
            <Comment>Host name of next server</Comment>
        </Term>
        <Term id="dhcp.new-client-ip">
            <Type>shortstring</Type>
            <Comment>New IP addr assigned to client</Comment>
        </Term>
        <Term id="dhcp.new-client-subnet">
            <Type>shortstring</Type>
            <Comment>New IP subnet assigned to client</Comment>
        </Term>
        <Term id="dhcp.gateway-ip">
            <Type>shortstring</Type>
            <Comment>IP addr of the gateway</Comment>
        </Term>
        <Term id="dhcp.option-type">
            <Type>uint32</Type>
            <Comment>Option type</Comment>
        </Term>
        <Term id="dhcp.option-value">
            <Type>blob</Type>
            <Comment>Content or value of the option</Comment>
        </Term>
        <Term id="dhcp.host-name">
            <Type>shortstring</Type>
            <Comment>Host name sent by the client in the DCHP option</Comment>
        </Term>
        <Term id="dhcp.domain-name">
            <Type>shortstring</Type>
            <Comment>DNS server name sent by the server in the DHCP option</Comment>
        </Term>
        <Term id="dhcp.client-fqdn">
            <Type>string</Type>
            <Comment>Fully qualified host name sent by the client in the DHCP option</Comment>
        </Term>

        <Term id="dhcp.remote-id-type">
            <Type>shortstring</Type>
            <Comment>Suboption that contains the remote agent identifier</Comment>
        </Term>
        <Term id="dhcp.remote-id-subtype">
            <Type>shortstring</Type>
            <Comment>Subtype for the remote agent</Comment>
        </Term>
        <Term id="dhcp.remote-id">
            <Type>shortstring</Type>
            <Comment>Identifier for the remote agent</Comment>
        </Term>
        <Term id="dhcp.circuit-id">
            <Type>shortstring</Type>
            <Comment>The circuit identifier</Comment>
        </Term>
        <Term id="dhcp.time-server">
            <Type>string</Type>
            <Comment>Time Servers</Comment>
        </Term>
        <Term id="dhcp.name-server">
            <Type>string</Type>
            <Comment>Name Servers</Comment>
        </Term>
        
        <Term id="dhcp.log-server">
            <Type>string</Type>
            <Comment>Logging Servers</Comment>
        </Term>
        <Term id="dhcp.quotes-server">
            <Type>string</Type>
            <Comment>Quotes Servers</Comment>
        </Term>
        <Term id="dhcp.printer-server">
            <Type>string</Type>
            <Comment>Printer Servers</Comment>
        </Term>
        <Term id="dhcp.impress-server">
            <Type>string</Type>
            <Comment>Impress Servers</Comment>
        </Term>
        <Term id="dhcp.rlp-server">
            <Type>string</Type>
            <Comment>RLP Servers</Comment>
        </Term>
        <Term id="dhcp.swap-server">
            <Type>string</Type>
            <Comment>Swap Servers</Comment>
        </Term>
        <Term id="dhcp.static-route">
            <Type>string</Type>
            <Comment>Static Routing Table</Comment>
        </Term>
        <Term id="dhcp.nis-server">
            <Type>string</Type>
            <Comment>NIS Servers</Comment>
        </Term>
        <Term id="dhcp.ntp-server">
            <Type>string</Type>
            <Comment>NTP Servers</Comment>
        </Term>
        <Term id="dhcp.netbios-server">
            <Type>string</Type>
            <Comment>Netbios Servers</Comment>
        </Term>
        <Term id="dhcp.xwin-font-server">
            <Type>string</Type>
            <Comment>X Windows Font Server</Comment>
        </Term>
        <Term id="dhcp.requested-ip">
            <Type>string</Type>
            <Comment>Client requested IP address</Comment>
        </Term>
        <Term id="dhcp.error-msg">
            <Type>string</Type>
            <Comment>DHCP Error Message</Comment>
        </Term>
        <Term id="dhcp.client-id">
            <Type>string</Type>
            <Comment>Client Identifier</Comment>
        </Term>
        <Term id="dhcp.nisv3-server">
            <Type>string</Type>
            <Comment>NIS V3 Server</Comment>
        </Term>
        <Term id="dhcp.tftp-server">
            <Type>string</Type>
            <Comment>TFTP Server</Comment>
        </Term>
        <Term id="dhcp.sip-server">
            <Type>string</Type>
            <Comment>SIP Server</Comment>
        </Term>
        
    </Vocabulary>
</CmConfig>
