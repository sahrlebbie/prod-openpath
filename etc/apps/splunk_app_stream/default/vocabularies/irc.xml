<?xml version="1.0" encoding="UTF-8"?>
<CmConfig xmlns="http://purl.org/cloudmeter/config" version="7.2.0">
  <Vocabulary id="irc">
    <Name>IRC Protocol Vocabulary</Name>
    <Locked>true</Locked>
      <Term id="irc.event">
          <Type>object</Type>
          <Comment>Event representing IRC</Comment>
      </Term>
      <Term id="irc.channel">
          <Type>shortstring</Type>
          <Comment>Name of the chat room</Comment>
      </Term>
      <Term id="irc.file-id">
          <Type>string</Type>
          <Comment>FIle correlation key</Comment>
      </Term>
      <Term id="irc.filename">
          <Type>string</Type>
          <Comment>Name of the transferred file</Comment>
      </Term>
      <Term id="irc.login">
          <Type>shortstring</Type>
          <Comment>User's login string</Comment>
      </Term>
      <Term id="irc.login-server">
          <Type>string</Type>
          <Comment>Concatenated login and server</Comment>
      </Term>
      <Term id="irc.message">
          <Type>string</Type>
          <Comment>Chat message</Comment>
      </Term>
      <Term id="irc.channel-name">
          <Type>string</Type>
          <Comment>Name of the IRC channel</Comment>
      </Term>
      <Term id="irc.mode-status">
          <Type>shortstring</Type>
          <Comment>Status of the IRC channel</Comment>
      </Term>
      <Term id="irc.nick-name">
          <Type>shortstring</Type>
          <Comment>User's alias'</Comment>
      </Term>
      <Term id="irc.receiver">
          <Type>shortstring</Type>
          <Comment>Contains the identity of the receiver for a chat message or a file transfer</Comment>
      </Term>
      <Term id="irc.sender">
          <Type>shortstring</Type>
          <Comment>Contains the identity of the sender of a chat session or a file transfer</Comment>
      </Term>
      <Term id="irc.server">
          <Type>shortstring</Type>
          <Comment>Server name to which the user is connected</Comment>
      </Term>
  </Vocabulary>
</CmConfig>
