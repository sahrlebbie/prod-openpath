<?xml version="1.0" encoding="UTF-8"?>
<CmConfig xmlns="http://purl.org/cloudmeter/config" version="7.2.0">
  <Vocabulary id="mount">
    <Locked>true</Locked>
    <Name>Mount Protocol Vocabulary</Name>
    <Term id="mount.event">
      <Type>object</Type>
      <Comment>Event representing a mount command</Comment>
    </Term>
    <Term id="mount.filehandle">
      <Type>blob</Type>
      <Comment>File handle metadata uncoded</Comment>
    </Term>
    <Term id="mount.filehandle-length">
      <Type>uint32</Type>
      <Comment>Length of the file handle</Comment>
    </Term>
    <Term id="mount.flavor">
      <Type>uint32</Type>
      <Comment>Authentification supported by the server</Comment>
    </Term>
    <Term id="mount.flavors">
      <Type>uint32</Type>
      <Comment>Number of authentification flavors supported by the server</Comment>
    </Term>
    <Term id="mount.path">
      <Type>shortstring</Type>
      <Comment>Value of the data path string</Comment>
    </Term>
    <Term id="mount.path-length">
      <Type>uint32</Type>
      <Comment>Length of the data path string</Comment>
    </Term>
  </Vocabulary>
</CmConfig>
