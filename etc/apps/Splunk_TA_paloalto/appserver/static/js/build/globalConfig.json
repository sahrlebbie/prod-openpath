            "services": [
                {
                    "name": "autofocus_export", 
                    "entity": [
                        {
                            "required": true, 
                            "type": "text", 
                            "label": "Name", 
                            "field": "name", 
                            "help": "Enter a unique name for the data input", 
                            "validators": [
                                {
                                    "pattern": "^[a-zA-Z]\\w*$", 
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "type": "regex"
                                }, 
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "type": "string", 
                                    "maxLength": 100
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "type": "text", 
                            "label": "Interval", 
                            "field": "interval", 
                            "help": "Time interval of input in seconds.", 
                            "validators": [
                                {
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$", 
                                    "errorMsg": "Interval must be an integer.", 
                                    "type": "regex"
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "type": "singleSelect", 
                            "label": "Index", 
                            "field": "index", 
                            "defaultValue": "default", 
                            "options": {
                                "blackList": "^_.*$", 
                                "createSearchChoice": true, 
                                "endpointUrl": "data/indexes"
                            }, 
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "type": "string", 
                                    "maxLength": 80
                                }
                            ]
                        }, 
                        {
                            "required": false, 
                            "type": "text", 
                            "label": "Label", 
                            "field": "label", 
                            "help": "", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }
                    ], 
                    "title": "AutoFocus Export"
                }, 
                {
                    "name": "aperture", 
                    "entity": [
                        {
                            "required": true, 
                            "type": "text", 
                            "label": "Name", 
                            "field": "name", 
                            "help": "Enter a unique name for the data input", 
                            "validators": [
                                {
                                    "pattern": "^[a-zA-Z]\\w*$", 
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "type": "regex"
                                }, 
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "type": "string", 
                                    "maxLength": 100
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "type": "text", 
                            "label": "Interval", 
                            "field": "interval", 
                            "help": "Time interval of input in seconds.", 
                            "validators": [
                                {
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$", 
                                    "errorMsg": "Interval must be an integer.", 
                                    "type": "regex"
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "type": "singleSelect", 
                            "label": "Index", 
                            "field": "index", 
                            "defaultValue": "default", 
                            "options": {
                                "blackList": "^_.*$", 
                                "createSearchChoice": true, 
                                "endpointUrl": "data/indexes"
                            }, 
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "type": "string", 
                                    "maxLength": 80
                                }
                            ]
                        },
                        {
                            "label": "Region",
                            "field": "region",
                            "defaultValue": "us",
                            "type": "singleSelect",
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "US",
                                        "value": "us"
                                    },
                                    {
                                        "label": "Europe",
                                        "value": "eu"
                                    },
                                    {
                                        "label": "Asia Pacific",
                                        "value": "apac"
                                    }
                                ],
                                "disableSearch": true
                            },
                            "required": true
                        },
                        {
                            "required": true, 
                            "type": "singleSelect", 
                            "label": "Global Account", 
                            "field": "global_account", 
                            "help": "", 
                            "options": {
                                "referenceName": "account"
                            }
                        }
                    ], 
                    "title": "Aperture"
                },
                {
                    "title": "MineMeld Feed", 
                    "name": "minemeld_feed",
                    "entity": [
                        {
                            "field": "name", 
                            "help": "Enter a unique name for the data input", 
                            "required": true, 
                            "validators": [
                                {
                                    "type": "regex", 
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "pattern": "^[a-zA-Z]\\w*$"
                                }, 
                                {
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "type": "string", 
                                    "maxLength": 100, 
                                    "minLength": 1
                                }
                            ], 
                            "type": "text", 
                            "label": "Name"
                        }, 
                        {
                            "field": "interval", 
                            "help": "Time interval of input in seconds.", 
                            "required": true, 
                            "validators": [
                                {
                                    "type": "regex", 
                                    "errorMsg": "Interval must be an integer.", 
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$"
                                }
                            ], 
                            "type": "text", 
                            "label": "Interval"
                        }, 
                        {
                            "field": "index", 
                            "validators": [
                                {
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "type": "string", 
                                    "maxLength": 80, 
                                    "minLength": 1
                                }
                            ], 
                            "required": true, 
                            "defaultValue": "default", 
                            "options": {
                                "blackList": "^_.*$", 
                                "endpointUrl": "data/indexes", 
                                "createSearchChoice": true
                            }, 
                            "type": "singleSelect", 
                            "label": "Index"
                        }, 
                        {
                            "field": "feed_url", 
                            "help": "", 
                            "required": true, 
                            "validators": [
                                {
                                    "errorMsg": "Max length of text input is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0
                                }
                            ], 
                            "type": "text", 
                            "label": "Output Node Feed URL"
                        }, 
                        {
                            "field": "indicator_timeout",
                            "help": "How long to retain indicators (in hours)",
                            "required": true,
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Interval must be an integer.",
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$"
                                }
                            ],
                            "type": "text",
                            "label": "Indicator Timeout"
                        },
                        {
                            "field": "credentials", 
                            "help": "", 
                            "required": false, 
                            "options": {
                                "referenceName": "account"
                            }, 
                            "type": "singleSelect", 
                            "label": "Feed Credentials"
                        }
                    ]
                }
            ], 
            "description": "Manage your data inputs", 
            "title": "Inputs"
        }, 
        "configuration": {
            "tabs": [
                {
                    "table": {
                        "header": [
                            {
                                "field": "name", 
                                "label": "Account name"
                            }, 
                            {
                                "field": "username", 
                                "label": "Username"
                            }
                        ], 
                        "actions": [
                            "edit", 
                            "delete", 
                            "clone"
                        ]
                    }, 
                    "name": "account", 
                    "entity": [
                        {
                            "required": true, 
                            "type": "text", 
                            "label": "Account name", 
                            "field": "name", 
                            "help": "Enter a unique name for this account.", 
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of ID should be between 1 and 50", 
                                    "type": "string", 
                                    "maxLength": 50
                                }, 
                                {
                                    "pattern": "^[a-zA-Z]\\w*$", 
                                    "errorMsg": "Account name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "type": "regex"
                                }
                            ]
                        }, 
                        {
                            "required": true, 
                            "type": "text", 
                            "label": "Username", 
                            "field": "username", 
                            "help": "Enter the username for this account.", 
                            "options": {
                                "placeholder": "Enter the username here"
                            }, 
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of username should be between 1 and 200", 
                                    "type": "string", 
                                    "maxLength": 200
                                }
                            ]
                        }, 
                        {
                            "encrypted": true, 
                            "required": true, 
                            "type": "text", 
                            "label": "Password", 
                            "field": "password", 
                            "help": "Enter the password for this account.", 
                            "validators": [
                                {
                                    "minLength": 1, 
                                    "errorMsg": "Length of password should be between 1 and 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }
                    ], 
                    "title": "Account"
                }, 
                {
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } return true; }"
                    }, 
                    "name": "proxy", 
                    "entity": [
                        {
                            "field": "proxy_enabled", 
                            "type": "checkbox", 
                            "label": "Enable"
                        }, 
                        {
                            "field": "proxy_type", 
                            "defaultValue": "http", 
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "http", 
                                        "value": "http"
                                    }, 
                                    {
                                        "label": "socks4", 
                                        "value": "socks4"
                                    }, 
                                    {
                                        "label": "socks5", 
                                        "value": "socks5"
                                    }
                                ], 
                                "disableSearch": true
                            }, 
                            "type": "singleSelect", 
                            "label": "Proxy Type"
                        }, 
                        {
                            "field": "proxy_url", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max host length is 4096", 
                                    "type": "string", 
                                    "maxLength": 4096
                                }
                            ], 
                            "type": "text", 
                            "label": "Host"
                        }, 
                        {
                            "field": "proxy_port", 
                            "validators": [
                                {
                                    "range": [
                                        1, 
                                        65535
                                    ], 
                                    "type": "number"
                                }
                            ], 
                            "type": "text", 
                            "label": "Port"
                        }, 
                        {
                            "field": "proxy_username", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of username is 50", 
                                    "type": "string", 
                                    "maxLength": 50
                                }
                            ], 
                            "type": "text", 
                            "label": "Username"
                        }, 
                        {
                            "field": "proxy_password", 
                            "encrypted": true, 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of password is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ], 
                            "type": "text", 
                            "label": "Password"
                        }, 
                        {
                            "field": "proxy_rdns", 
                            "type": "checkbox", 
                            "label": "Reverse DNS resolution"
                        }
                    ], 
                    "title": "Proxy"
                }, 
                {
                    "name": "logging", 
                    "entity": [
                        {
                            "field": "loglevel", 
                            "defaultValue": "INFO", 
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "DEBUG", 
                                        "value": "DEBUG"
                                    }, 
                                    {
                                        "label": "INFO", 
                                        "value": "INFO"
                                    }, 
                                    {
                                        "label": "WARNING", 
                                        "value": "WARNING"
                                    }, 
                                    {
                                        "label": "ERROR", 
                                        "value": "ERROR"
                                    }, 
                                    {
                                        "label": "CRITICAL", 
                                        "value": "CRITICAL"
                                    }
                                ], 
                                "disableSearch": true
                            }, 
                            "type": "singleSelect", 
                            "label": "Log level"
                        }
                    ], 
                    "title": "Logging"
                }, 
                {
                    "name": "additional_parameters", 
                    "entity": [
                        {
                            "encrypted": true, 
                            "type": "text", 
                            "label": "AutoFocus API Key", 
                            "field": "autofocus_api_key", 
                            "defaultValue": "", 
                            "help": "Used to retrieve metadata about AutoFocus tags. Requires a MineMeld Feed input to download threat indicators. More info: https://splunk.paloaltonetworks.com/autofocus-and-minemeld.html",
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of password is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }, 
                        {
                            "encrypted": true, 
                            "type": "text", 
                            "label": "WildFire API Key", 
                            "field": "wildfire_api_key", 
                            "defaultValue": "", 
                            "help": "Used to retrieve reports from the WildFire Cloud.  An API Key is available from the WildFire Portal. https://wildfire.paloaltonetworks.com",
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of password is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ]
                        }
                    ], 
                    "title": "Add-on Settings"
                }
            ], 
            "description": "Set up your add-on", 
            "title": "Configuration"
        }
    }
}
