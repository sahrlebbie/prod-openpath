<dashboard theme="dark">
  <label>Squid Proxy Dashboard</label>
  <description>Squid Proxy Sample Logs</description>
  <row>
    <panel>
      <single>
        <search>
          <query>index=*access* sourcetype=squid_proxy_access proxy_status=*HIT* 
| stats count(proxy_status) as proxy_sucesses by src_ip
| stats count</query>
          <latest>1184472000</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">block</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0x53a051"]</option>
        <option name="rangeValues">[0]</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="underLabel">Proxy Successes</option>
        <option name="unitPosition">after</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <single>
        <search>
          <query>index=*access* sourcetype=squid_proxy_access proxy_status=*miss* 
| stats count(proxy_status) as proxy_misses by src_ip 
| sort - proxy_misses
| where proxy_misses &gt;1000 
| stats count</query>
          <latest>1184472000</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">block</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0xf8be34","0xdc4e41"]</option>
        <option name="rangeValues">[0,50]</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="underLabel">Host Proxy Misses &gt;1000</option>
        <option name="unitPosition">after</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
    <panel>
      <single>
        <search>
          <query>index=*access* sourcetype=squid_proxy_access proxy_status=*miss* 
| stats count(proxy_status) as proxy_misses by src_ip 
| sort - proxy_misses
| where proxy_misses &gt;2000
| stats count</query>
          <latest>1184472000</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorMode">block</option>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0x53a051","0xf1813f","0xdc4e41"]</option>
        <option name="rangeValues">[0,100]</option>
        <option name="underLabel">Host Proxy Misses &gt;2000</option>
        <option name="useColors">1</option>
      </single>
    </panel>
    <panel>
      <single>
        <search>
          <query>index=*access* sourcetype=squid_proxy_access proxy_status=*miss* 
| stats count(proxy_status) as proxy_misses by src_ip 
| sort - proxy_misses
| where proxy_misses &gt;3000
| stats count</query>
          <latest>1184472000</latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">block</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0xdc4e41"]</option>
        <option name="rangeValues">[0]</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="underLabel">Host Proxy Misses &gt;3000</option>
        <option name="unitPosition">after</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <title>Proxy Status Count</title>
      <chart>
        <search>
          <query>index=*access* sourcetype=squid_proxy_access 
