<dashboard theme="dark">
  <label>mimi access</label>
  <row>
    <panel>
      <title>Attempts</title>
      <chart>
        <title>Action</title>
        <search>
          <query>index=openpath_eventgen sourcetype=WinEventLog | stats count as attempts by action</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.chart">pie</option>
        <option name="charting.drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
    <panel>
      <title>App</title>
      <chart>
        <title>App</title>
        <search>
          <query>index=openpath_eventgen sourcetype=WinEventLog |chart count by category, app usenull=false</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.chart">column</option>
        <option name="charting.drilldown">none</option>
      </chart>
    </panel>
  </row>
</dashboard>